{
  "name": "@deeep-network/riptide",
  "version": "0.2.11",
  "description": "Universal Riptide Runtime for the DeEEP Network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "riptide": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc --noEmit && tsup",
    "build:publish": "tsc --noEmit && tsup --minify",
    "build:docker": "./scripts/build-docker.sh",
    "deploy:docker": "./scripts/deploy-docker.sh",
    "deploy:npm": "./scripts/deploy-npm.sh",
    "test:universal": "./tests/test-universal.sh",
    "release": "./scripts/release.sh",
    "release:minor": "./scripts/release.sh --type minor",
    "release:major": "./scripts/release.sh --type major",
    "release:dry-run": "./scripts/release.sh --dry-run",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "@types/node": "^22.14.0",
    "tsup": "^8.5.0"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deeep-network/riptide.git"
  },
  "keywords": [
    "container",
    "entrypoint",
    "lifecycle",
    "hooks",
    "coral-reef",
    "riptide"
  ],
  "author": "DeEEP Network",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  }
}
